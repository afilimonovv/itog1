pipeline {
    agent any
    tool name: 'terraform101', type: 'terraform'
    stages {
        stage ('git check') {
            steps {
                git 'https://github.com/afilimonovv/itog1.git'
            }
        }
        stage ('terraforminit') {
            steps {
                sh 'terraform init'
                sh 'terraform --version'
            }
        }
        stage ('terraformApply') {
            steps {
                sh 'terraform apply'
            }
        }
    }
}